digraph Flowchart {
  graph [splines=ortho];
  node [fontname="Helvetica", fontsize=10, style=rounded];
  edge [fontname="Helvetica", fontsize=9];
  node0 [label="Start", shape=ellipse];
  node1 [label="n := INPUT(\"Enter a positive integer (n):\")", shape=box];
  node2 [label="result := 1", shape=box];
  node3 [label="NOT n <= 0", shape=diamond];
  node4 [label="", shape=point, width="0", height="0"];
  node5 [label="", shape=point, width="0", height="0"];
  node6 [label="", shape=point, width="0", height="0"];
  node7 [label="n <= 1", shape=diamond];
  node8 [label="", shape=point, width="0", height="0"];
  node9 [label="break_start", shape=point, width="0", height="0"];
  node9 -> node6 [];
  node10 [label="break_end", shape=point, width="0", height="0"];
  node7 -> node9 [xlabel="True"];
  node10 -> node8 [];
  node7 -> node8 [xlabel="False"];
  node11 [label="result := result * n", shape=box];
  node12 [label="n := n - 1", shape=box];
  node8 -> node11 [];
  node11 -> node12 [];
  node5 -> node7 [];
  node12 -> node5 [];
  node3 -> node5 [xlabel="True"];
  node6 -> node4 [];
  node3 -> node4 [xlabel="False"];
  node13 [label="OUTPUT: result", shape=parallelogram];
  node1 -> node2 [];
  node2 -> node3 [];
  node4 -> node13 [];
  node0 -> node1 [];
  node14 [label="End", shape=ellipse];
  node13 -> node14 [];
}
